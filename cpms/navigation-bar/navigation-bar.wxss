


.container {
    width: 100%;
    position: fixed; /*脱离布局 */
    top: 0;
    z-index: 999;
}

.container .status-bar {
    
}

.container .nav-bar {
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative; /*子绝父相*/
}

.nav-bar .left-item {
    position: absolute;
    left: 0;
    top: 0;
    height: 100%;
    display: flex;
    align-items: center;
    padding: 0 30rpx;
}
.left-item .icon {
    width: 36rpx;
    height: 36rpx;
}
.left-item .back {

}
.left-item .home {
    margin-left: 35rpx;
}

.nav-bar .nav-title {
    font-size: 35.5rpx;
    font-weight: 480;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}